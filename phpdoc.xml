<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
    <title>MCStreetguy/Crawler</title>
    <parser>
        <target>Docs/cache</target>
        <encoding>utf8</encoding>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <default-package-name>mcstreetguy/crawler</default-package-name>
    </parser>
    <transformer>
        <target>Docs/html</target>
    </transformer>
    <transformations>
        -- Generate html documentation
        <template name="responsive-twig" />
        -- Generate markdown docs for GitHub
        <template name="vendor/mcstreetguy/phpdoc-markdown-public/data/templates/markdown-public" />
    </transformations>
    <files>
        <directory>Classes</directory>
        <ignore>Tests/*</ignore>
    </files>
</phpdoc>