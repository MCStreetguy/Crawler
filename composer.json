{
    "name": "mcstreetguy/crawler",
    "description": "An advanced web-crawler written in PHP.",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "readme": "./README.md",
    "require": {
        "guzzlehttp/guzzle": "~6.0",
        "guzzlehttp/psr7": "^1.5",
        "nicmart/tree": "^0.2.7",
        "webmozart/assert": "^1.4",
        "ramsey/uuid": "^3.8",
        "paquettg/php-html-parser": "^2.0"
    },
    "require-dev": {
        "kint-php/kint": "^3.2",
        "phpdocumentor/phpdocumentor": "^2.9"
    },
    "suggest": {
        "ext-curl": "For improved network requests, replaces stream handler usage."
    },
    "authors": [
        {
            "name": "Maximilian Schmidt",
            "email": "maximilianschmidt404@gmail.com",
            "homepage": "https://www.mcstreetguy.de/",
            "role": "Lead-Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "MCStreetguy\\Crawler\\": "Classes/"
        }
    }
}
